//Извлечь элемент (по указанному индексу) из заданного списка.
import java.util.ArrayList;
import java.util.List;
import java.util.Random;
class Main {
  public static void main(String[] args) {
    List<Integer> arr1 = new ArrayList<>();
    for(int i = 0; i < 5; i++ ){
      arr1.add(new Random().nextInt(25));
    }
    
    System.out.println("Полученный список: " + arr1);
    int element = arr1.get(2);
    System.out.println("Элемент списка по индексу: " + element);
  }
}
